<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" 
	xmlns:layout="http://www.sbml.org/sbml/level3/version1/layout/version1"
	xmlns:render="http://www.sbml.org/sbml/level3/version1/render/version1"
	level="3" version="1">
	
	<model id="small">
		<listOfUnitDefinitions>
			<unitDefinition id="volume">
				<listOfUnits>
					<unit kind="litre" exponent="1" scale="0" multiplier="1" />
				</listOfUnits>
			</unitDefinition>
			<unitDefinition id="substance">
				<listOfUnits>
					<unit kind="mole" exponent="1" scale="0" multiplier="1" />
				</listOfUnits>
			</unitDefinition>
			<unitDefinition id="area">
				<listOfUnits>
					<unit kind="metre" exponent="2" scale="0" multiplier="1" />
				</listOfUnits>
			</unitDefinition>
		</listOfUnitDefinitions>
		
		<listOfCompartments>
			<compartment id="Cell" name="Cytosol" spatialDimensions="3" units="volume" constant="true" />
		</listOfCompartments>
		
		<listOfSpecies>
		  <species id="A" name="A" compartment="Cell" 
		    substanceUnits="substance" hasOnlySubstanceUnits="false"
			boundaryCondition="false" constant="false" />
		  <species id="B" name="B" compartment="Cell" 
		    substanceUnits="substance" hasOnlySubstanceUnits="false"
			boundaryCondition="false" constant="false" />
		</listOfSpecies>
		
		<listOfReactions>
		  <reaction id="r" reversible="false" fast="false">
			<listOfReactants>
			  <speciesReference id="Ref_A" species="A" stoichiometry="1" constant="true" />
			</listOfReactants>
			<listOfProducts>
			  <speciesReference id="Ref_B" species="B" stoichiometry="1" constant="true" />
			</listOfProducts>
		  </reaction>
		</listOfReactions>
		
		
		<layout:listOfLayouts xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		  xmlns:layout="http://www.sbml.org/sbml/level3/version1/layout/version1">
			<layout:layout layout:id="The">
				<layout:dimensions layout:width="400" layout:height="300" />
				<layout:listOfCompartmentGlyphs>
				  <layout:compartmentGlyph layout:id="CompartmentGlyph_Cell" layout:compartment="Cell">
				    <layout:boundingBox layout:id="bbc">
					  <layout:position layout:x="10" layout:y="80" />
					  <layout:dimensions layout:width="380" layout:height="140" />
					</layout:boundingBox>
				  </layout:compartmentGlyph>
				</layout:listOfCompartmentGlyphs>
				<layout:listOfSpeciesGlyphs>
				  <layout:speciesGlyph layout:id="SpeciesGlyph_A" layout:species="A">
					<layout:boundingBox layout:id="bb1">
					  <layout:position layout:x="50" layout:y="150" />
					  <layout:dimensions layout:width="50" layout:height="50" />
					</layout:boundingBox>
				  </layout:speciesGlyph> 
				  <layout:speciesGlyph layout:id="SpeciesGlyph_B" layout:species="B">
					<layout:boundingBox layout:id="bb2">
					  <layout:position layout:x="300" layout:y="150" />
					  <layout:dimensions layout:width="80" layout:height="40" />
					</layout:boundingBox>
				  </layout:speciesGlyph> 
				</layout:listOfSpeciesGlyphs>
				
				<layout:listOfReactionGlyphs>
				  <layout:reactionGlyph layout:id="ReactionGlyph_r" layout:reaction="r">
					<layout:boundingBox layout:id="bbr1">
					  <layout:position layout:x="190" layout:y="190" />
					  <layout:dimensions layout:width="20" layout:height="40" />
				    </layout:boundingBox>
					<layout:curve>
					  <layout:listOfCurveSegments>
						<layout:curveSegment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="LineSegment">
							<layout:start layout:x="180" layout:y="210"/>
							<layout:end layout:x="220" layout:y="210"/>
						</layout:curveSegment>
					  </layout:listOfCurveSegments>
					</layout:curve>
					<layout:listOfSpeciesReferenceGlyphs>
						<layout:speciesReferenceGlyph layout:id="SpeciesReferenceGlyph_A" 
						  render:objectRole="substrate" layout:speciesReference="Ref_A" 
						  layout:speciesGlyph="SpeciesGlyph_A" layout:role="substrate">
						  <layout:curve>
							<layout:listOfCurveSegments>
							  <layout:curveSegment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="LineSegment">
								<layout:start layout:x="180" layout:y="210" />
								<layout:end layout:x="100" layout:y="175" />
							  </layout:curveSegment>
							</layout:listOfCurveSegments>
						  </layout:curve>
						</layout:speciesReferenceGlyph>
						
						<layout:speciesReferenceGlyph layout:id="SpeciesReferenceGlyph_B" 
						  render:objectRole="product" layout:speciesReference="Ref_B" 
						  layout:speciesGlyph="SpeciesGlyph_B" layout:role="product">
						  <layout:curve>
							<layout:listOfCurveSegments>
							  <layout:curveSegment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="LineSegment">
								<layout:start layout:x="220" layout:y="210" />
								<layout:end layout:x="300" layout:y="175" />
							  </layout:curveSegment>
							</layout:listOfCurveSegments>
						  </layout:curve>
						</layout:speciesReferenceGlyph>
					</layout:listOfSpeciesReferenceGlyphs>
				  </layout:reactionGlyph>
				</layout:listOfReactionGlyphs>
				
				<layout:listOfTextGlyphs>
				  <layout:textGlyph layout:id="TextGlyph_A" layout:originOfText="A" layout:graphicalObject="SpeciesGlyph_A">
				    <layout:boundingBox layout:id="bbtA">
					  <layout:position layout:x="50" layout:y="150" />
					  <layout:dimensions layout:width="50" layout:height="50" />
					</layout:boundingBox>
				  </layout:textGlyph>
				  <layout:textGlyph layout:id="TextGlyph_B" layout:originOfText="B" layout:graphicalObject="SpeciesGlyph_B">
				    <layout:boundingBox layout:id="bbtB">
					  <layout:position layout:x="300" layout:y="150" />
					  <layout:dimensions layout:width="50" layout:height="50" />
					</layout:boundingBox>
				  </layout:textGlyph>
				  <layout:textGlyph layout:id="TextGlyph_Cell" layout:originOfText="Cell" layout:graphicalObject="CompartmentGlyph_Cell">
				    <layout:boundingBox layout:id="bbtCell">
					  <layout:position layout:x="10" layout:y="80" />
					  <layout:dimensions layout:width="50" layout:height="50" />
					</layout:boundingBox>
				  </layout:textGlyph>
				</layout:listOfTextGlyphs>
				
				<render:listOfRenderInformation xmlns:render="http://www.sbml.org/sbml/level3/version1/render/version1"
				 render:versionMajor="1" render:versionMinor="0">
					<render:renderInformation render:id="SBGN" render:programName="Ralph Gauges" 
					 render:programVersion="1.0" render:backgroundColor="#FFFFFFFF"> 
					  <render:listOfColorDefinitions>
					    <render:colorDefinition render:id="black" render:value="#000000" />
						<render:colorDefinition render:id="red" render:value="#FF0000" />
						<render:colorDefinition render:id="white" render:value="#ffffff" />
					  </render:listOfColorDefinitions>
					  
					  <render:listOfLineEndings>
						<render:lineEnding xmlns:layout="http://www.sbml.org/sbml/level3/version1/layout/version1" render:id="productionHead"> 
						<layout:boundingBox>
						  <layout:position layout:x="-9" layout:y="-5" /> 
						  <layout:dimensions layout:width="10" layout:height="10" /> 
						</layout:boundingBox> 
						<render:g render:stroke="black" render:stroke-width="1" render:fill="black">
						  <render:polygon render:stroke="black" render:stroke-width="0.3" render:fill="white">
							<render:listOfElements>
							  <render:element xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="RenderPoint" render:x="0" render:y="5" />
							  <render:element xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="RenderPoint" render:x="5" render:y="0" />
							  <render:element xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="RenderPoint" render:x="10" render:y="5" />
							  <render:element xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="RenderPoint" render:x="5" render:y="10" />
						    </render:listOfElements>
						  </render:polygon>
						</render:g>
						</render:lineEnding>
					  </render:listOfLineEndings>
					  
					  <render:listOfStyles>
						<render:style render:id="PerturbingAgentStyle" render:idList="SpeciesGlyph_B">
						  <render:g>
							<render:polygon render:stroke="black" render:stroke-width="1" render:fill="white">
							  <render:listOfElements>
								<render:element render:x="0" render:y="0" xsi:type="RenderPoint"/>
								<render:element render:x="11.6" render:y="20" xsi:type="RenderPoint"/>
								<render:element render:x="0" render:y="40" xsi:type="RenderPoint"/>
								<render:element render:x="80" render:y="40" xsi:type="RenderPoint"/>
								<render:element render:x="68.4" render:y="20" xsi:type="RenderPoint"/>
								<render:element render:x="80" render:y="0" xsi:type="RenderPoint"/>
							  </render:listOfElements>
							</render:polygon>
						  </render:g>
						</render:style>
					  
						<render:style render:id="SimpleChemicalStyle" render:idList="SpeciesGlyph_A">
						  <render:g render:stroke="black" render:stroke-width="3"
						   render:fill-rule="nonzero" render:font-size="12"
						   render:font-family="sans-serif" render:font-style="normal"
						   render:font-weight="normal" render:text-anchor="middle"
						   render:vtext-anchor="middle">
						    <!-- Note: COPASI does not render this correctly. As by the render-specification (p.33), 
							     specifying a relative rx and ratio=1 should always yield a circle. -->
							<render:ellipse render:cx="50%" render:cy="50%" render:rx="50%" render:ratio="1" />
							<!-- <render:text render:x="0" render:y="0"
							 render:font-family="monospace" render:text-anchor="middle"
							 render:vtext-anchor="middle">?</render:text> -->
						  </render:g>
						</render:style>
						
						<render:style render:id="reactionGlyphStyle" render:idList="ReactionGlyph_r">
						  <render:g render:stroke="black" render:stroke-width="1">
							<render:rectangle render:x="5" render:y="15"
							 render:width="10" render:height="10" render:fill="white"/>
							<render:text render:x="0" render:y="0" render:transform="0.5, 0.8660254, -0.8660254, 0.5, 15.4102540, 9.3708349"
							 render:font-size="10"
							 render:font-family="monospace" render:text-anchor="start" render:vtext-anchor="top">?</render:text>
						  </render:g>
						</render:style>
						
						<render:style render:id="productSpeciesReferenceGlyphStyle"
						 render:roleList="product sideproduct">
						  <!-- For the non-SBML-roles (catalysis, necessary activation), can use the idList instead -->
						  <render:g render:stroke="black" render:stroke-width="2"
						   render:fill-rule="nonzero" render:font-size="0" 
						   render:font-family="sans-serif" render:font-style="normal" 
						   render:font-weight="normal" render:endHead="productionHead" />
						</render:style>
					  </render:listOfStyles>
					</render:renderInformation>
				</render:listOfRenderInformation>
			</layout:layout>
		</layout:listOfLayouts>
	</model> 
</sbml> 